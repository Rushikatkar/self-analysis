    <h2 class="heading">User's Information</h2>
    <br>
    <div>
        <table>
            
            <tr>
                <th>name</th>
                <th>email</th>
                <th>password</th>
                <th>marks</th>
                <th>cgpa</th>
                <th>backlogs</th>
                <th>domain</th>
                <th>Send Notification to</th>
            </tr>
            <% if(company.length > 0){ %>
                <% for(var i = 1; i < company.length; i++){ %>
                        <tr>
                            <td><%= company[i].name %></td>
                            <td> <%= company[i].email %> </td>
                            <td> <%= company[i].password %> </td>
                            <td> <%= company[i].marks %> </td>
                            <td> <%= company[i].cgpa %> </td>
                            <td> <%= company[i].backlogs %> </td>
                            <td> <%= company[i].domain %> </td>
                            <td><a href="/notification"><%= company[i].name %></a></td>
                        </tr>
                <% } %>
            <% } %>
                    
